        function updateFile(id) {
            /*Get filename*/
            var filename = $(id).val();
            /*Get filename and extension without complete path*/
            var lastIndex = filename.lastIndexOf("\\");
            if (lastIndex >= 0) {
                filename = filename.substring(lastIndex + 1);
            }
            /*If no file selected*/
            if (filename == "") {
                filename = " No file chosen";
            }
            /*Show/update selected filename*/
            $("#PortalTheme_wt28_block_wtcontent_holder_forumsCore_wtPosts_block_PortalTheme_wt96_block_wtfilename2").html(" " + filename);
        }
        
        /*When selected file is changed*/
        $(_this).change(function() {
            updateFile(_this);
        });
        
        if ("" != "") {
            $("#PortalTheme_wt28_block_wtcontent_holder_forumsCore_wtPosts_block_PortalTheme_wt96_block_wtfilename2").html(" " + "");
        }
    });
